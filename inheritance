#include<iostream>
using namespace std;
class vehicle{           //base class
	public:
		string brand="ford";
		void honk()
		{
			cout<<"tuut,tuut! \n";
		}
	};
	class car:public vehicle       // derived class
		{
			public:
				string model ="mustang";
		
};
int main()
{
	car myobj;
	myobj.honk();
cout<<myobj.brand +" "+ myobj.model<<"\n";
return 0;
	
}
